[colors]
background = #2e3440
background1 = #ee2e3440
foreground = #81a1c1
background-alt = #dd81a1c1
foreground-alt = #2e3440
primary = #ebcb8b
secondary = #d08770
alert = #bf616a


[bar/mainbar0]
inherit = bar/barbase
modules-left = ewmh

[bar/mainbar1]
inherit = bar/barbase1
modules-left = ewmh


[bar/barbase]
width = ${env:width}
offset-x = ${env:offsetx}
monitor = ${env:monitor}
;offset-y = ${env:y}
;width = 95%
height = 24
fixed-center = false
background = ${colors.background}
foreground = ${colors.foreground}
line-size = 3
line-color = #f00
border-size = 0
border-color = #00000000
locale = fr_CA.UTF-8
padding-left = 0
padding-right = 1
module-margin = 0
font-0 = DaddyTimeMono Nerd Font:style=Regular:size=12:antialias=true:hinting=true;3
font-1 = Font Awesome 5 Free Solid:style=Solid:pixelsize=12;3
font-2 = DaddyTimeMono Nerd Font Mono:style=regular:size=16:antialias=true:hinting=true;3
font-3 = Weather Icons:style=Regular:size=14;3
font-4 = Font Awesome 5 Brands Regular:style=Solid:pixelsize=12;3
modules-center =
modules-right = home photo lsepgreen cpu-all memory temperature lsep81a1c1 meteo info-airqualityindex lsepgreen pulseaudio lsepyellow date-fr
tray-position = right
tray-padding = 0
cursor-click = pointer
cursor-scroll = ns-resize

[bar/barbase1]
width = ${env:width}
offset-x = ${env:offsetx}
monitor = ${env:monitor}
;offset-y = ${env:y}
;width = 100%
height = 24
fixed-center = true
background = ${colors.background1}
foreground = ${colors.foreground}
line-size = 3
line-color = #f00
border-size = 0
border-color = #00000000
locale = fr_CA.UTF-8
padding-left = 0
padding-right = 0
module-margin-left = 0
module-margin-right = 0
font-0 = DaddyTimeMono Nerd Font:style=Regular:size=12:antialias=true:hinting=true;3
font-1 = Font Awesome 5 Free Solid:style=Solid:pixelsize=12;3
font-2 = DaddyTimeMono Nerd Font Mono:style=regular:size=16:antialias=true:hinting=true;3
font-3 = Weather Icons:style=Regular:size=12;3
font-4 = Font Awesome 5 Brands Regular:style=Solid:pixelsize=12;3
modules-center = 
modules-right = cpu-all lsep81a1c1 updates-apt lsepred pulseaudio lsepyellow date2
tray-position = 
tray-padding = 
cursor-click = pointer
cursor-scroll = ns-resize

[module/leftwmicon]
type = custom/text
content = %{T5}%{T-}

[module/lsep81a1c1]
type = custom/text
content = %{T3}%{F#81a1c1}┃%{F-}%{T-}

[module/lsepred]
type = custom/text
content = %{T3}%{F#bf616a}┃%{F-}%{T-}


[module/lsepgreen]
type = custom/text
content = %{T3}%{F#a3be8c}┃%{F-}%{T-}

[module/lsepviolet]
type = custom/text
content = %{T3}%{F#b48ead}┃%{F-}%{T-}

[module/lsepyellow]
type = custom/text
content = %{T3}%{F#ebcb8b}┃%{F-}%{T-}

[module/rsep]
type = custom/text
content = %{T3}%{F#81a1c1}┃%{F-}%{T-}

[fs-base]
type = internal/fs

; Default: 90
; New in version 3.6.0
warn-percentage = 75

label-mounted-foreground = #a3be8c
label-mounted-background = ${colors.background}
label-unmounted = %mountpoint% not mounted
label-unmounted-foreground = ${colors.foreground-alt}

[module/home]
inherit = fs-base
mount-0 = /home

label-mounted = " / %percentage_used%%/%used% "

[module/photo]
inherit = fs-base

mount-0 = /home/mdupuis/Photographie

label-mounted = "-- Photo %percentage_used%%/%used% "


[module/mpd]
type = internal/mpd
format-online = " <label-time>   <label-song> <icon-prev> <icon-stop> <toggle> <icon-next>  "
format-offline = " <label-offline>  "

; Only applies if <icon-X> is used
icon-play = 
icon-pause = 
icon-stop = 
icon-prev =  
icon-next = 

; Available tokens:
;   %artist%
;   %album-artist%
;   %album%
;   %date%
;   %title%
; Default: %artist% - %title%
label-song =  %artist% - %title%
label-song-maxlen = 25
label-song-ellipsis = true

label-offline = " mpd is offline"


[module/cpu]
type = internal/cpu
interval = 2
format-prefix = " "
format-prefix-foreground = #81a1c1
;format-underline = #88c0d0
label = "%{T2}%{T-} %percentage-cores:2% "
format-foreground = #81a1c1
format-background = ${colors.background}

[module/cpu-all]
type = internal/cpu
interval = 2
format-prefix = " "
format-prefix-foreground = #81a1c1
;format-underline = #88c0d0
label = "%{T2}%{T-} %percentage%% "
format-foreground = #81a1c1
format-background = ${colors.background}

[module/memory]
type = internal/memory
interval = 2
format-prefix = " "
format-prefix-foreground = #81a1c1
;format-underline = #88c0d0
label = "%{T2}%{T-} %percentage_used%% "
format-foreground = #81a1c1
format-background = ${colors.background}

[module/system-fan-speed]
type = custom/script
exec = ~/.config/leftwm/themes/md-nord/polybar-scripts/system-fan-speed.sh
interval = 10
format-foreground = #81a1c1
format-background = ${colors.background}


[module/network]
type = internal/network
interface = enp3s0
interval = 3.0

;format-connected-underline = #a3be8c
label-connected-foreground = #b48ead
label-connected-background = ${colors.background}
;format-connected-prefix = " "
;format-connected-prefix-foreground = ${colors.foreground-alt}
label-connected = " %{T3}%{T-} %downspeed% %{T3}%{T-} %upspeed% "

format-disconnected =
;format-disconnected = <label-disconnected>
;format-disconnected-underline = #bf616a
;label-disconnected = %ifname% disconnected
;label-disconnected-foreground = ${colors.foreground-alt}

[module/date-fr]
type = custom/script
exec = ~/.config/leftwm/themes/md-nord/polybar-scripts/getdate_fr.py
; Seconds to sleep between updates
; Default: 5 (0 if `tail = true`)
interval = 1.0

format = <label>
format-foreground = #eceff4
format-background = ${colors.background}

label = "  %output%  "


[module/date1]
type = internal/date
date = " %%{F#eceff4}%A, %d %b %Y ‒ %k:%M:%%{F#EBCB8B}%S%%{F-} "
date-alt = "  %k:%M  "
format-foreground = #eceff4
format-background = ${colors.background}


[module/date2]
type = internal/date
date = "  %k:%M  "
date-alt = " %%{F#eceff4}%A, %d %b %Y ‒ %k:%M:%%{F#ebcb8b}%S%%{F-} "
format-foreground = #eceff4
format-background = ${colors.background}

[module/pulseaudio]
type = internal/pulseaudio

format-volume-foreground = #ebcb8b
format-volume-background = ${colors.background}
format-foreground = #ebcb8b
format-background = ${colors.background}
format-volume = %{T2} <ramp-volume>%{T-}<label-volume>

label-volume-foreground = #ebcb8b
label-volume-background = ${colors.background}
label-volume = " %percentage%% "
;label-volume-foreground = ${root.foreground}

label-muted = " %{T2}婢%{T-} " 
label-muted-foreground = ${colors.alert}
label-muted-background = #ebcb8b

;bar-volume-width = 10
;bar-volume-foreground-0 = #55aa55
;bar-volume-foreground-1 = #55aa55
;bar-volume-foreground-2 = #55aa55
;bar-volume-foreground-3 = #55aa55
;bar-volume-foreground-4 = #55aa55
;bar-volume-foreground-5 = #f5a70a
;bar-volume-foreground-6 = #ff5555
;bar-volume-gradient = false
;bar-volume-indicator = |
;bar-volume-indicator-font = 2
;bar-volume-fill = ─
;bar-volume-fill-font = 2
;bar-volume-empty = ─
;bar-volume-empty-font = 2
;bar-volume-empty-foreground = ${colors.foreground-alt}

; Only applies if <ramp-volume> is used
ramp-volume-0 = 
ramp-volume-1 = 
ramp-volume-2 = 
ramp-volume-foreground = #ebcb8b
ramp-volume-background = ${colors.background}

[module/temperature]
type = internal/temperature
thermal-zone = 2
base-temperature = 55
warn-temperature = 70

format = <ramp><label>
format-foreground = #81a1c1
format-background = ${colors.background}
;format-underline = #88c0d0

format-warn = <ramp> <label-warn>
format-warn-foreground = ${colors.alert}
format-warn-background = ${colors.background}
;format-warn-underline = ${colors.secondary}

label = " %temperature-c% "
label-warn = " %temperature-c% "
;label-warn-foreground = ${colors.foreground-alt}
;label-warn-background = ${colors.alert}

ramp-0 = 
ramp-1 = 
ramp-2 = 
;ramp-foreground = #81a1c1

[module/meteo]
type = custom/script
exec = ~/.config/leftwm/themes/md-nord/polybar-scripts/openweathermap-fullfeatured.sh
format-foreground = #a3be8c
format-background = ${colors.background}
;format-underline = #5e81ac
label = " %output% "
interval = 600
label-font = 4

[module/info-airqualityindex]
type = custom/script
exec = ~/.config/leftwm/themes/md-nord/polybar-scripts/info-airqualityindex.sh
format-foreground = #a3be8c
format-background = ${colors.background}
;label = " %output% "
interval = 600
label-font = 4

[module/ewmh]
type = internal/xworkspaces

; Only show workspaces defined on the same output as the bar
;
; Useful if you want to show monitor specific workspaces
; on different bars
;
; Default: false
pin-workspaces = false

; Create click handler used to focus desktop
; Default: true
enable-click = true

; Create scroll handlers used to cycle desktops
; Default: true
enable-scroll = false

;tags = ["", "磊", "", "", "", "", "", ""]
icon-1 = 1;
icon-2 = 2;磊
icon-3 = 3;
icon-4 = 4;
icon-5 = 5;
icon-6 = 6;
icon-7 = 7;
icon-8 = 8;
;icon-9 = 9;
;icon-0 = 0;
icon-10 = scratchpad;
icon-default = " "


format = <label-state> 
label-monitor = %icon% 

label-active =  %icon%
label-active-foreground = ${colors.foreground}
label-active-background = ${colors.background-alt}
label-active-padding = 2

;label-occupied = %icon%
label-occupied-foreground = ${colors.foreground-alt}
label-occupied-background = ${colors.background}
label-occupied-padding = 2

label-urgent = %icon%
label-urgent-foreground = #FF0000
label-urgent-background = ${colors.background}
;label-urgent-underline = ${colors.alert}
label-urgent-padding = 2


label-empty = %icon%
label-empty-foreground = #008000
label-enpty-background = #00000000
label-empty-padding = 2


[module/updates-apt]
type = custom/script
exec = ~/.config/leftwm/themes/md-nord/polybar-scripts/updates-apt.sh
format = <label>
format-foreground = #bf616a
format-background = ${colors.background}
;format-underline = #5e81ac

label = "  %output% "
label-fail = "  cmd failed "
interval = 21600

click-left = ~/.config/leftwm/themes/md-nord/polybar-scripts/updates-apt.sh


[settings]
screenchange-reload = true
;compositing-background = xor
;compositing-background = screen
;compositing-foreground = source
;compositing-border = over
;pseudo-transparency = false

;[global/wm]
;margin-top = 5
;margin-bottom = 5

; vim:ft=dosini
